html { font-family: "Helvetica Neue", Helvetica, STHeiTi, Arial, sans-serif; -ms-text-size-adjust: 100%; -webkit-text-size-adjust: 100%; }
body { margin: 0;  -webkit-user-select: none;  font-size: 12px; line-height: 1; color: #666; background-color: #fff;}
article, footer, header,  nav, section,pre { display: block; }
audio, canvas, progress, video { display: inline-block; vertical-align: baseline; }
ul, ol, li, dl, dd, div, h1, h2, h3, h4, h5, h6, p, form, td, th{ margin: 0; padding: 0; }
ul, ol { list-style: none; }
body, a, img{ -webkit-touch-callout: none;}
a {text-decoration: none; -webkit-tap-highlight-color:rgba(0,0,0,0); color: #55c1e7; outline: 0; font-size: 12px;}
a:active{outline: 0;}
small { font-size: 80%; }
img { border: 0; vertical-align: middle; }
h1,h2,h3,h4,h5{color: #333;}
button, input,  select, textarea, label{color: inherit; font: inherit; margin: 0;  padding: 0; -webkit-tap-highlight-color:rgba(0,0,0,0); }
button:focus{ outline: 0;} 
label{font-size:14px; color: #333; -webkit-tap-highlight-color:rgba(0,0,0,0);}
button, select { text-transform: none; }
button, html input[type="button"], input[type="reset"], input[type="submit"] { -webkit-appearance: none; cursor: pointer; border-radius: 0;}
button[disabled], html input[disabled] { cursor: default; }
button::-moz-focus-inner, input::-moz-focus-inner { border: 0; padding: 0; }
input,select{font-size: 14px; line-height: normal;}
input[type="checkbox"], input[type="radio"] {padding: 0; }
input[type="number"]::-webkit-inner-spin-button, input[type="number"]::-webkit-outer-spin-button { height: auto; }
input[type="search"] {-webkit-appearance: textfield; }
input[type="search"]::-webkit-search-cancel-button, input[type="search"]::-webkit-search-decoration { -webkit-appearance: none; }
table { border-collapse: collapse; border-spacing: 0; }
input:-ms-input-placeholder, textarea:-ms-input-placeholder { color: #ccc; }
input::-webkit-input-placeholder, textarea::-webkit-input-placeholder { color: #ccc; }
*, *:after, *:before { -webkit-box-sizing: border-box; box-sizing: border-box;}


/*项目图标*/
@font-face {font-family: "iconfont";
  src: url('../font/iconfont.eot'); /* IE9*/
  src: url('../font/iconfont.eot?#iefix') format('embedded-opentype'),
  url('../font/iconfont.woff') format('woff'), 
  url('../font/iconfont.ttf') format('truetype'),
  url('../font/iconfont.svg#iconfont') format('svg'); /* iOS 4.1- */
}

[class^="icon-"]:before, [class*=" icon-"]:before{font-family:"iconfont" !important; font-style:normal; -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2px; -moz-osx-font-smoothing: grayscale; display: inline-block;}
.icon-close:before { content: "\e602"; }
.icon-right:before { content: "\e603"; }
.icon-round-check:before { content: "\e604"; }
.icon-round-check-fill:before { content: "\e605"; }
.icon-round-close:before { content: "\e606"; }


/*字体颜色*/
.warning{color: #f00;}

/*上下间距*/
.mt15{margin-top: 15px;}
.mt50{margin-top: 50px;}

/**/
.btn{width: 100%; display: block; padding:0 40px; border:none; background-color: transparent; height: 42px; line-height: 42px; border-radius: 6px; font-size: 20px; color: #fff; text-align: center; }
.btn-default{border:1px solid #bbb; background-color: #fff; color: #666;}/*白色*/
.btn-default:active{border-color: #7a7a7a;}
.btn-primary{background-color: #ff814a;}/*首选橙色*/
.btn-primary:active{background-color: #fd773d;}
.btn-info{background-color: #55c1e7;}/*一般信息蓝色*/
.btn-info:active{background-color: #51b8dc;}
.btn-warning{background-color: #c91002;}/*警告*/
.btn-warning:active{background-color: #bb1003;}
.btn-info-ghost{border:1px solid #55c1e7; color: #55c1e7; }
.btn-info-ghost:active{border-color: #51b8dc; color: #51b8dc;}
.btn-sm{height: 30px; line-height: 30px; padding: 0 30px; border-radius: 4px; font-size: 14px;}
.btn-xs{height: 24px; line-height: 24px; padding: 0 20px; border-radius: 3px; font-size: 12px;}
.btn-noRadius{border-radius: 0}
.btn-loading{background-color: #ccc; color: #fff;}
.btn-primary[disabled],
.btn-primary.disabled,
.btn-primary[disabled]:active,
.btn-primary.disabled:active,{background-color: #ff814a; color: #ffb398;}

.space{height: 6px; overflow: hidden; background-color: #f2f2f2; }
.box-bordered{border-top: 1px solid #e8e8e8; border-bottom: 1px solid #e8e8e8;}/*上下带边框的盒子*/
.box-bgcolor{background-color: #fff;}/*带白底的盒子*/

