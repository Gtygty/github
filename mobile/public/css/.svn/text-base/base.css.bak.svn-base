.container:after,.row:after,.row-full:after,.clearfix:after{display: block; clear: both; visibility: hidden; height: 0; overflow:hidden; content:" ";}


/*弹性盒模型*/
.form-group,
.label-act,
.label-act>*,
.user .container,
.popup-ft,
.label-act .info,
.label-info
{
	width: 100%;
	display: box;			  /* OLD - Android 4.4- */
	display: -webkit-box;	  /* OLD - iOS 6-, Safari 3.1-6 */
	display: -moz-box;		 /* OLD - Firefox 19- (buggy but mostly works) */
	display: -ms-flexbox;	  /* TWEENER - IE 10 */
	display: -webkit-flex;	 /* NEW - Chrome */
	display: flex;	
	-webkit-box-orient: horizontal;
	-webkit-flex-direction: row;
	-moz-flex-direction: row;
	-ms-flex-direction: row;
	-o-flex-direction: row;
	flex-direction: row;
	-webkit-box-align: center;
	-webkit-align-items: center;
	-moz-align-items: center;
	-ms-align-items: center;
	-o-align-items: center;
	align-items: center;
}

.form-control,
.label-act .info,
.label-info .txt,
.pop-slide-bd,
.user-txt,
.popup-ft>button{
	-webkit-box-flex: 1; 
	-moz-box-flex:1; 
	-webkit-flex: 1; 
	-ms-flex: 1; 
	flex: 1;
}
.popup-ft{
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	-moz-justify-content: center;
	-ms-justify-content: center;
	-o-justify-content: center;
	justify-content: center;
}
.label-info{
	-webkit-box-align: start ;
	-webkit-align-items: flex-start ;
	-moz-align-items: flex-start ;
	-ms-align-items: flex-start ;
	-o-align-items: flex-start ;
	align-items: flex-start ;
}


/*form-group*/
.form-group{position: relative; padding-top:4px; padding-bottom: 4px; border-bottom: 1px solid #e8e8e8; width: 100%;}
.form-label{font-size: 14px; line-height: 34px; color: #333; width: 60px; margin-right: 12px;}
.form-control{border:none; height:34px; background-color: transparent; padding:0 32px 0 6px;}
.form-group > [class*=icon]{width: 24px; height: 24px; line-height:24px; font-size: 16px; color: #bbb; position: absolute; top: 50%; margin-top: -12px; right: 5px; text-align: center;}


/*登录*/
.login-photo{width: 18%; max-width: 120px; margin: 0 auto; padding-top: 32px; padding-bottom: 20px;}
.login-photo img{ display: block; width: 100%;}


/*个人中心*/
.user{background-color: #ff814a; color: #fff; line-height: 18px; padding-top: 12px; padding-bottom: 12px; }
.user-photo{width: 60px; height: 60px; overflow: hidden;}
.user-photo a,
.user-photo img{display: block; width: 100%; height: 100%; position: relative;}
.user-photo img{border-radius: 50%;}
.user-photo a:after{content:''; width: 18px; height: 18px; position: absolute; right: 0; bottom: 0; border-radius: 50%; background-color: rgba(0,0,0,.4);}
.user-txt{margin-left: 12px;}


.label-act{border-bottom: 1px solid #e8e8e8; padding-left: 12px; padding-right: 12px; line-height: 18px;}
.label-act>a{width: 100%; padding-top: 12px; padding-bottom: 12px;}
.label-act:nth-last-of-type(1){border-bottom: none;}
.label-act .tit{width: 40%;}
.label-act .info{padding-left: 12px; padding-right:10px; color: #999; word-break:break-all;
	-webkit-justify-content: flex-end;
	-moz-justify-content: flex-end;
	-ms-justify-content: flex-end;
	-o-justify-content: flex-end;
	justify-content: flex-end;}
.label-act .tit{font-size: 14px; color:#666;}
.label-act .ico{width: 16px;}
.label-act .arrow{width: 9px;}

.label-info{border-bottom: 1px solid #e8e8e8; line-height: 18px; padding-top: 12px; padding-bottom: 12px;}
.label-info label{width: 70px; color: #666;}
.label-info .txt{font-size: 14px; word-break:break-all;}


/*弹层*/
.popup-mask{background-color: rgba(0,0,0,.3); position: fixed; width: 100%; height: 100%; top: 0; left: 0; -webkit-transition:.2s opacity; transition:.2s opacity; display: none; opacity: 0; z-index: 10;}

.pop-slide-con{ padding:0 12px 12px; position: fixed; background-color: #fff; bottom: 0; left: 0; width: 100%; z-index: 11;
	-webkit-transform:translateY(100%); 
	transform:translateY(100%); 
	-webkit-transition:.2s; 
	transition:.2s;
}
.pop-slide-hd{line-height: 20px; text-align: center; padding: 16px 40px; border-bottom: 1px solid #e8e8e8;}
.pop-slide-hd h3{ font-size: 16px;}
.pop-slide-hd p{ color: #666;}
.pop-slide-bd{padding-bottom: 16px; }
.pop-slide-ft{padding-top: 10px;}
.pop-slide-hd .close{position: absolute; left: 12px; top: 16px; font-size: 24px; width: 24px; height: 24px; line-height: 24px; overflow: hidden; color: #bbb;}

.popup-container{width: 72%; position: fixed; top: 40%; left: 50%; display: none; opacity: 0; text-align: center; z-index: 999;
	-webkit-transform:translateX(-50%) translateY(-50%); 
	transform:translateX(-50%) translateY(-50%);
	 -webkit-transition: .2s opacity; 
	 transition: .2s opacity; }
.popup-con{width: 100%; background-color: #fff; border-radius: 6px; overflow: hidden; }
.popup-bd{padding: 20px; text-align: center; line-height: 18px;}
.popup-ft>button{height: 44px; line-height: 44px; border: none; background-color: #fff; border-right: 1px solid #e8e8e8; color: #2181f7; text-align: center;} 
.popup-ft>button:active{color: #0072ff;}
.popup-ft>button:nth-last-of-type(1){border-right: none;}
.popup-ft{border-top:1px solid #e8e8e8;}
.popup-con-notfiy{ display: inline-block; width: auto; padding: 0 15px; background-color: rgba(0,0,0,0.8); color: #fff;}