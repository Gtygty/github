    </view>
    <footer>
        <div class="flex-box border-t footer">
            <div class="flex1"><a href="index.html" title=""><i class="icon-home icon-lg"></i>关爱商城</a></div>
            <div class="flex1"><a href="collect.html" title=""><i class="icon-favorites icon-lg"></i>收藏夹</a></div>
            <div class="flex1 factive"><a href="cart.html" title=""><i class="icon-cart icon-lg"></i>购物车</a></div>
            <div class="flex1"><a href="order.html" title=""><i class="icon-order icon-lg"></i>订单</a></div>
        </div>
    </footer>

    <!-- 左侧列表滑出 -->
    <div class="class-mask"></div>
    <section class="classify" style="left: 100%;">
        <div class="classright">
            <a class="classback" href="#" title="">点 击 此 处 返 回<i class="icon-roundback fs-14"></i></a>
        </div>
        <div class="classleft of-s bg-f">
            <div class="classleft-cont">
                <div class="hotwords border-b pb28 mb28">
                    <h6>分类</h6>
                    <span>全部分类</span>
                    <span>保温杯</span>
                    <span>手机壳</span>
                    <span>床品四件套</span>
                    <span>移动电源</span>
                    <span>收纳整理</span>
                </div>
                <div class="hotwords border-b pb28 mb28">
                    <h6>品牌</h6>
                    <span>全部品牌</span>
                    <span>保温杯</span>
                    <span>手机壳</span>
                    <span>床品四件套</span>
                    <span>移动电源</span>
                    <span>收纳整理</span>
                </div>
                <div class="pop-slide-btn flex-box border-t">
                    <a href="#" class="flex1 show footbtn-default" id="cancel" title="">取消</a>
                    <a href="#" class="flex1 show footbtn1" id="sure" title="">确定</a>
                </div>
            </div>
        </div>
    </section>
</body>
<script type="text/javascript" >
$(function(){
        
    //左侧滑动
    function hindden(){
        $('.classify').animate({left: '100%'}, 'slow');
        $('.class-mask').fadeOut();
        $('view').css('overflow-y', 'auto');
    }
    $('.search label').eq(1).click(function(event) {
        $('.classify').animate({left: 0}, 'slow');
        $('.class-mask').fadeIn();
        $('view').css('overflow-y', 'hidden');
    });
    $('.classback , #cancel').bind('click', function(event) {
        hindden();
    });
    $('.classleft-cont .hotwords').each(function(i) {
        $('.classleft-cont .hotwords').eq(i).find('span').click(function(event) {
            $(this).addClass('wordsactive').siblings().removeClass('wordsactive');
        });
    });
    $('#sure').bind('click', function(){
        var a = $('.classleft-cont').find('.wordsactive').length;
        if (a > 0) {
            hindden();
            $('.search-right').addClass('fc-primary').find('i').addClass('fc-primary');
        };
    })

    //收缩
    $('#search').focus(function(event) {
        $('.hot-search').fadeIn();
        $('#screening').addClass('hide-i')
        $('#cancel').removeClass('hide-i');
        $('#classify').fadeOut();
        $('view').addClass('border-n');
        $('view').css('overflow-y', 'hidden');
        $('footer').hide();
    });
    $('#cancel').click(function(event) {
        $('#classify').fadeIn();
        $('.hot-search').fadeOut();
        $('#cancel').addClass('hide-i');
        $('#screening').removeClass('hide-i');
        $('view').removeClass('border-n');
        $('view').css('overflow-y', 'auto');
        $('footer').show();
    });
})
</script>
</html>