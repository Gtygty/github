<script type="text/javascript">
$(function(){
    //左侧滑动
    function hindden(){
        $('.classify').animate({left: '100%'}, 'slow');
        $('.class-mask').fadeOut();
        $('view').css('overflow-y', 'auto');
    }
    $('.search label').eq(1).click(function(event) {
        $('.classify').animate({left: 0}, 'slow');
        $('.class-mask').fadeIn();
        $('view').css('overflow-y', 'hidden');
    });
    $('.classright , #cancel').bind('click', function(event) {
        hindden();
    });
    $('.classleft-cont .hotwords').each(function(i) {
        $('.classleft-cont .hotwords').eq(i).find('span').click(function(event) {
            $(this).addClass('wordsactive').siblings().removeClass('wordsactive');
        });
    });
    $('#sure').bind('click', function(){
        var a = $('.classleft-cont').find('.wordsactive').length;
        if (a > 0) {
            hindden();
            $('.search-right').addClass('fc-primary').find('i').addClass('fc-primary');
        };
    })
    

    //懒人加载
    $(function() {
      $("img.lazy").lazyload({effect: "fadeIn"});
    });

    //排行点击
    var san = 0;
    var $sorting = $('section.ranklist div.flex1');
    $sorting.bind('click', function() {
        var index = $sorting.index(this);
        if (!$(this).hasClass('rankactve')) {
            san = 1;
        };
        $(this).addClass('rankactve').siblings().removeClass('rankactve');
        $('section.ranklist i').removeClass('dsanactive usanactive')
        if (san == 0) {
            $(this).find('i').eq(1).addClass('dsanactive');
            san = 1;
        }else{
            $(this).find('i').eq(0).addClass('usanactive');
            san = 0;
        };

        function listshow(sex){
            $('ul.shoplist li').each(function(i){
                $('ul.shoplist li').eq(i).find('label').eq(sex).show().siblings('label').hide();
            })
        }
        if (index == 0) {
            listshow(0);
        };
        if (index == 1) {
            listshow(1);
        };
        if (index == 2) {
            listshow(0);
        };
    });

    //关注
    $('ul.shoplist li').find('i.icon-like').bind('click',function(){
        if ($(this).hasClass('icon-likefill')) {
            return;
        }else{
            $(this).removeClass('icon-like').addClass('icon-likefill');
            var num = $(this).parent().find('span');
            var val = parseFloat(num.text());
            val++;
            num.html(val);
        };

    });

    //收缩
    $('#search').focus(function(event) {
        $('.hot-search').fadeIn();
        $('#screening').addClass('hide-i')
        $('#cancel').removeClass('hide-i');
        $('#classify').fadeOut();
        $('view').addClass('border-n');
        $('view').css('overflow-y', 'hidden');
        $('footer').hide();
    });
    $('#cancel').click(function(event) {
        $('#classify').fadeIn();
        $('.hot-search').fadeOut();
        $('#cancel').addClass('hide-i');
        $('#screening').removeClass('hide-i');
        $('view').removeClass('border-n');
        $('view').css('overflow-y', 'auto');
        $('footer').show();
    });

    var loadSwitch = true;
    var canLoad = false;
    var page = 0;
    var myScroll = new IScroll('#wrapScroll',{
        tap: true,
        click:true,
        scrollbars: true,               //显示滚动条
        interactiveScrollbars: true,    //此属性可以让滚动条能拖动，用户可以与之交互。
        shrinkScrollbars: 'scale',      //当在滚动区域外面滚动时滚动条是否可以收缩到较小的尺寸。
        fadeScrollbars: true            //不想使用滚动条淡入淡出方式时，需要设置此属性为false以便节省资源。
    });
    document.addEventListener('touchmove',function(ev){ev.preventDefault()});
    $('.classleft-cont')[0].addEventListener('touchmove',function(ev){ev.stopPropagation()});
})
</script>
