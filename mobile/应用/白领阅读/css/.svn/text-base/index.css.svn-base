.contain_rd .detail p:nth-child(4) {
	font-size: 12px;
	color: #999999;
	text-overflow: -o-ellipsis-lastline;
	overflow: hidden;
	text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-line-clamp: 2;
	-webkit-box-orient: vertical;
	line-height: 20px;
}
.contain_rd .detail p:nth-child(5) {
	font-size: 11px;
	color: #999;
}
.contain_rd .detail a{
	width: 200px;
	height: 100%;
	background: url(../img/jump.png) no-repeat center right;
	margin-right: 10px;
	background-size: 11px 20px;
	position: absolute;
	right: 0px;
	top: 0px;
}
.comment_rd {
	width: 100%;
	background: #f7f7f7;
	overflow: hidden;
	padding: 0px 11px 0px 11px;
	box-sizing: border-box;
	background: #fff;
}
._h4 {
	font-size: 15px;
	color: #666666;
	line-height: 100%;
	display: block;
    padding-left: 12px;
    padding-top: 8px;
    background-color: #fff;
}
.comment_rd .comment_cont {
	margin-top: 20px;
	padding-bottom: 20px;
	border-bottom: 1px solid #eee;
	overflow: hidden;
}
.comment_rd .comment_cont h3{
	font-size: 16px;
	line-height: 100%;
	color: #666;
}
.comment_rd .comment_cont p{
	font-size: 10px;
	color: #cccccc;
	line-height: 14px;
	padding-bottom: 13px;
}
.comment_rd .comment_cont span {
	font-size: 13px;
	color: #999999;
	table-layout:fixed; 
	word-break: break-all; 
	overflow:hidden; 
}
.comment_rd .comment_cont:last-child {
	/*margin-bottom: 48px;*/
	border-bottom: none;
}
.coms_rd{
	width: 100%;
	height: auto;
	position: fixed;
	left: 0px;
	bottom: 0px;
	padding: 10px 17px 10px 20px;
	box-sizing: border-box;
	background: #f7f7f7;
	border-top: 1px solid #c9c9c9;
	z-index: 3;
}
.coms_rd .cons_cont{
	width: 100%;
}
.coms_rd textarea {
	height: 28px;
	width: 86%;
	float: left;
	border: none;
	border: 1px solid #C9C9C9;
	padding-left: 5px;
	font-size: 14px;
	color: #999;
	line-height: 20px;
	padding-top: 3px;
	box-sizing: border-box;
	transition:height 0.5s;
}
.coms_rd a {
	width: 20px;
	height: 21px;
	background: url(../img/comment.png) no-repeat;
	background-size: contain;
	float: right;
	margin-top: 4px;
}
.coms_rd textarea.cur {
	width: 100%;
	height: 60px;
}
.coms_rd a.cur {
	width: 60px;
	height: 30px;
	margin-top: 10px;
	background: url(../img/send.png) no-repeat;
	background-size: contain;
}

/*加载更多*/
