/*reset css*/
body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,fieldset,legend,input,textarea,p,blockquote,th,td{
	margin:0;
	padding:0;
}
html{
	color:#000;
}
body{
	font-size: 12px;
}
table{
	border-collapse:collapse;
	border-spacing:0;
}
fieldset,img{
	border:0;
}
address,caption,cite,code,dfn,em,strong,th,var{
	font-style:normal;
	font-weight:normal;
}
ol,ul{
	list-style:none;
}
caption,th{
	text-align:left;
}
h1,h2,h3,h4,h5,h6{
	font-size:100%;
	font-weight:normal;
}
q:before,q:after{
	content:'';
}
abbr,acronym{
	border:0;
	font-variant:normal;
}
sup{
	vertical-align:text-top;
}
sub{
	vertical-align:text-bottom;
}
input,textarea,select{
	font-family:inherit;
	font-size:inherit;
	font-weight:inherit;
	*font-size:100%;
}
legend{
	color:#000;
}
a{
	text-decoration: none;
	-webkit-tap-highlight-color: rgba(0,0,0,0);
}
input{
	border-radius: 0;
	-webkit-tap-highlight-color: rgba(0,0,0,0);
}
div,p,span,a,img,button,input,textarea{
	-webkit-tap-highlight-color:rgba(255,255,255,0);
}
input:focus, textarea:focus {
    outline: none;
}
a,div,p,input, textarea{
	-webkit-appearance: none; 
}
input::-webkit-inner-spin-button {
	-webkit-appearance: none;
}
input::-webkit-outer-spin-button {
	-webkit-appearance: none;
}
p, span {
	table-layout:fixed; 
	word-break: break-all;
}
.cb{
	clear: both;
}
html,body{
	background: #f2f2f2;
}
/*public*/
header {
	width: 100%;
	height: 36px;
	background: #00d685;
	position: fixed;
	left: 0px;
	top: 0px;
	right: 0px;
	z-index: 11111;
}
header p {
	float: left;
	width: 30%;
	height: 36px;
	background: url(../img/sidebar.png) no-repeat 20px center ;
	background-size: 24px 20px;
	cursor: pointer;
}
header h1 {
	width: 70%;
	font-size: 18px;
	line-height: 36px;
	color: #fff;
	text-align: center;
}
.kong {
	width: 100%;
	height: 36px;
}


/*侧边栏*/
.sidebar_bg {
	position: fixed;
	top: 0;
	left: 0;
	right: 0;
	bottom: 0;
	background: rgba(0,0,0,.4);
	z-index: 4;
	display: none;
}
.sidebar {
	width: 68%;
	position: fixed;
	top: 0px;
	left: -90%;
	bottom: 0px;
	background: #fff;
	padding-top: 64px;
	box-sizing: border-box;
	z-index: 2;
	transition: left 0.5s; 
	-webkit-transition: left 0.5s;
	z-index: 5;
}
.sidebar.cur{
	left: 0px;
}
.sidebar .box {
	width: 82%;
	margin:0 auto;
	overflow: hidden;
}
.sidebar a{
	display: block;
	width: 50%;
	height: auto;
	border-bottom: 1px solid #E0E0E0;
	border-right: 1px solid #E0E0E0;
	float: left;
	background: url(../img/sidebar_icons1.png) no-repeat center 16px;
	background-size: 50px 47px;
	padding-top: 76px;
	padding-bottom: 10px;
	text-align: center;
	font-size: 15px;
	color: #656571;
	box-sizing: border-box;
}
.sidebar a:nth-child(2) {
	border-right: none;
	background: url(../img/sidebar_icons2.png) no-repeat center 16px;
	background-size: 50px 47px;
}
.sidebar a:nth-child(3) {
	border-bottom: none;
	background: url(../img/sidebar_icons3.png) no-repeat center 16px;
	background-size: 50px 47px;
}
.sidebar a:nth-child(1).cur{
	background: #00D685 url(../img/sidebar_icons1_cur.png) no-repeat center 16px;
	background-size: 50px 47px;
	color: #fff;
}
.sidebar a:nth-child(2).cur{
	background: #00D685 url(../img/sidebar_icons2_cur.png) no-repeat center 16px;
	background-size: 50px 47px;
	color: #fff;
}
.sidebar a:nth-child(3).cur{
	background: #00D685 url(../img/sidebar_icons3_cur.png) no-repeat center 16px;
	background-size: 50px 47px;
	color: #fff;
}
/*dynamic css*/
.swipe {
	overflow: hidden;
	position: relative;
	background: url(../img/bg-swipe.png) no-repeat;
	-webkit-background-size: 100% auto;
  	background-size: 100% auto;
  	height: 0;
  	padding-bottom: 54.133333%;
}

.swipe-wrap {
	overflow: hidden;
	position: absolute;
	left: 0;
	top: 0;
	width: 100%;
	bottom: 0;
}
.swiper-container{ padding: 16px 0; }
.swipe-wrap li{
	width: 30%;
	float: left;
	box-shadow: 0 0 5px 2px rgb(218,243,234);
}
.swipe-wrap img{
	width: 100%;
}
.swiper-slide a{ display: block; line-height: 0; }
.swiper-slide{ -webkit-transition:.2s; transition:.2s; transform: translateX(10%) scale(0.85);}
.swiper-slide-active { transform: translateX(-10%) scale(0.85);}
.swiper-slide-next {  transform:scale(1);}
.xiaoyuandian{
	width: auto;
	height: 6px;
	position: absolute;
	bottom: 8%;
	left: 50%;
	transform:translateX(-50%);
	-webkit-transform:translateX(-50%);
}
.xiaoyuandian ul{
	list-style: none;
}
.xiaoyuandian ul li{
	float: left;
	width: 8px;
	height: 8px;
	background:#fff;
	margin-right: 8px;
	border-radius: 8px;
	opacity: 0.5;
}
.xiaoyuandian ul li:last-child{
	margin-right: 0px;
}
.xiaoyuandian ul li.cur{
	opacity: 1;
}
