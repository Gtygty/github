.bg-f{ background: #fff;}
.fc-f{ color: #fff;}
.hide{ display: none;}
.show{ display: block;}
.mt6{ margin-top: 6px; }
.money{ position: absolute; right: 10px; top: 8px; font-size: 14px;}
.tabulation{ position: fixed; right: 12px; bottom: 28px; width: 40px; height: 40px; border-radius: 50%; background: #ff814a; text-align: center; line-height: 40px; font-size: 20px;}
.prepaid{ position: fixed; width: 100%; height: 44px; background: #fff; bottom: 0; left: 0; text-align: center;}
.prepaid .btn{ width: auto; display: inline-block;}
.phone-prepaid .coupon-img{ background: url(../img/phone.png) no-repeat; -webkit-background-size: 100% auto; background-size: 100% auto;}
.phone-prepaid .coupon-txt p:nth-of-type(2){ margin-top: 16px; overflow: hidden;}
.phone-title{ height: 44px; font-size: 14px; color: #333;}