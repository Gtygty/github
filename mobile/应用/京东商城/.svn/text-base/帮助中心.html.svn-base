</body>
<script type="text/javascript">
$(function(){
    $('.help').on('click',function(){
        if ($(this).parent().hasClass('help-show')) {
            $(this).parent().removeClass('help-show');
            return;
        }
        $(this).parent().addClass('help-show');
        $(this).parent().siblings().removeClass('help-show')
    })
})
</script>
