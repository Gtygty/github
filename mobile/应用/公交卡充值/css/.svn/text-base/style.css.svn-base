.amount-list{ padding: 15px 6px 0; background: #fff;}
.amount-list li{ padding:0 6px; float: left; width: 33.3333%; margin-bottom: 15px;}
.amount{ line-height: 44px; height: 44px; border:1px solid #ccc; text-align: center; cursor: pointer; position: relative; border-radius: 4px;}
.amount-list .active .amount{ color: #ff7f00; border-color: #ff7f00;}
.amount-list .disabled .amount{ line-height: 16px; font-size: 12px; color: #fff; background: #ededed; border-color: #ededed; padding-top: 5px;}
.amount i{ width: 13px; height: 11px; background: url(../img/triangle.png) no-repeat; position: absolute; bottom: 0; right: 0; display: none;}
.amount-list .active .amount i{ display: block;}
.tabulation{ position: fixed; right: 12px; bottom: 28px; width: 40px; height: 40px; border-radius: 50%; background: #ff814a; text-align: center; line-height: 40px; font-size: 20px;}
.problems-list li{ padding:12px 0;}
.car-img{ width: 48px; height: 48px; margin-right: 10px; overflow: hidden; background-size: 100% auto; background-image: url(../img/carimg.png);}
.car-label h2{ color: #333;}
.no-order .icon-order{ width: 110px; height: 110px; text-align: center; line-height: 110px; background: #ddd; border-radius: 50%; display: inline-block; font-size: 74px;}
.question .label-act-left{ width: 90%;}

.ellipsis{ display: box; display: -webkit-box; box-orient: vertical; -webkit-box-orient: vertical; text-overflow: ellipsis; overflow: hidden; word-break: break-all;}
.ellipsis1 { line-clamp: 1; -webkit-line-clamp: 1;}