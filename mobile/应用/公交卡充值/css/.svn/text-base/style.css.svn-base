.bg-f{ background: #fff;}
.bg-f7{ background: #f7f7f7;}
.bg-prompt{ background: #fff7b3;}
.fc-f{ color: #fff;}
.fc-info{ color: #55c1e7;}
.dark{color: #333;}
.ht8{ height: 8px;}
.hide{ display: none;}
.show{ display: block;}
.vm{ vertical-align: middle;}
