<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="Cache-Control" content="no-siteapp" /><!-- 百度禁止转码 -->
    <meta name ="viewport" content ="width=device-width, initial-scale=1, maximum-scale=1, minimum-scale=1, user-scalable=no"> 
    <meta name="format-detection" content="telephone=no,email=no" /> <!-- 禁止自动识别为电话号码和邮件 -->
    <meta name="keywords" content="这里写关键字">
    <meta name="description" content="这里写页面描述">
    <title>个人中心-体检报销信息</title>
    <script type="text/javascript" src="../../public/js/jquery.js" ></script>
    <script type="text/javascript" src="js/height.js" ></script>
    <link rel="stylesheet" href="css/phone.css" />
    <link rel="stylesheet" href="css/theme.css" />
</head>
<body>
    <view class="border-n">
        <div class="medical-title flex-box">
            <div class="flex1 tc">
                <img src="img/medical.png" alt="" width="45" height="45">
                <p>体检报销流程</p>
            </div>
        </div>
        <div class="container">
            <ul class="medical-list border-b">
                <li class="flex-box border-b">
                    <div class="medical-cion step1"></div>
                    <div class="flex1">
                        <p>STEP 1</p>
                        <p class="ellipsis1">体检完毕并获取发票</p>
                    </div>
                </li>
                <li class="flex-box border-b">
                    <div class="medical-cion step2"></div>
                    <div class="flex1">
                        <p>STEP 2</p>
                        <p class="ellipsis1">在线填写报销信息</p>
                    </div>
                </li>
                <li class="flex-box border-b">
                    <div class="medical-cion step3"></div>
                    <div class="flex1">
                        <p>STEP 3</p>
                        <p class="ellipsis1">寄送发票原件并填写快递信息</p>
                    </div>
                </li>
                <li class="flex-box border-b">
                    <div class="medical-cion step4"></div>
                    <div class="flex1">
                        <p>STEP 4</p>
                        <p class="ellipsis1">寄送发票原件并填写快递信息</p>
                    </div>
                </li>
                <li class="flex-box border-b">
                    <div class="medical-cion step5"></div>
                    <div class="flex1">
                        <p>STEP 5</p>
                        <p class="ellipsis1">提交报销申请</p>
                    </div>
                </li>
            </ul>

            <div class="row-full mt44 mb20 tc">
                <a class="btn btn-default per45 d-ib mr8" id="btn-submit2" href="#" title="">我要体检</a>
                <a class="btn btn-primary per45 d-ib ml8" id="btn-submit" href="#" title="">我要报销</a>
            </div>
        </div>
    </view>
</body>
<script src="../../public/js/gat.AlertMessage.js"></script>
<script>
window.onload= function(){
    /*弹层效果*/
    var chooseHtml = '<h3 class="tc">温馨提示</h3>'
                    +'<ol class="promptlist">'
                        +'<li>选择体检中心，请您选择专业的体检中心或公立医院。</li>'
                        +'<li>预约体检时间，并准时进行体检。 </li>'
                        +'<li>稳妥保存体检发票，以备在后续报销时所用</li>'
                        +'<li>在线申请体检报销</li>'
                    +'</ol>';
    var chooseHtml2 = '<h3 class="tc">温馨提示</h3>'
                        +'<p class="fc-6 fs-12 pt16">您没有可使用的体检券</p>';
    $('#btn-submit').click(function(){
        message.showMessage({
            'btnTxt':'我知道了',
            'content':chooseHtml
        });
    })
    $('#btn-submit2').click(function(){
        message.showMessage({
            'btnTxt':'我知道了',
            'content':chooseHtml2
        });
    })
}
</script>
</html>