<section class="main-location" ng-controller="LocationController">
    <a href="#" class="userPhoto">
        <img src="img/photo.png" width="30" height="30" alt="">
    </a>
    <section class="menu border-b flex">
        <div class="menu-item active">快车</div>
        <div class="menu-item">专车</div>
        <span class="menu-line"></span>
    </section>
    <section class="container call-taxi-content">
        
        <h2 class="welcome tc">您好，您现在要去哪？</h2>
        
        <div class="location">
            <div class="hook from-hook border-b">
                <i class="i-circle i-from"></i>
                <span class="txt-hook ellipsis default">{{loc.startLoc.displayname}}</span>
            </div>
            <div class="hook to-hook border-b">
                <i class="i-circle i-to"></i>
                <span class="txt-hook ellipsis default">{{loc.endLoc.displayname}}</span>
            </div>
        </div>
        <div class="choose-type border-b flex">
            <div class="type-item active">
                <i class="i-cosy"></i>
                舒适型
            </div>
            <div class="type-item">
                <i class="i-commercial"></i>
                七座商务
            </div>
            <div class="type-item">
                <i class="i-deluxe"></i>
                豪华型
            </div>
            <em class="triangle border-l border-b"></em>
        </div>    
        <div class="tc mt16 clearfix">
            <p>本次行程约 <span class="price f24">50</span> 元</p>
        </div>
        <div class="mt16 tips row"><i class="icon-round-uninstall primary"></i>对不起，您当前没有足够的余额支付本单！</div>
        <button class="btn btn-primary mt16" id="calltaxi">呼叫快车</button>
        <div class="tc mt16"><a href="#">计价规则</a></div>
    </section>
</section>

<section class="pop-slide-up">
    <section class="pop-slide-con">
        <div class="pop-slide-hd row">
            <h3>选择付款方式</h3>
            <i class="icon-round-close close"></i>
        </div>
        <div class="pop-slide-bd">
            <div class="m-radio choosePayType">
                <label class="u-radio border-b disabled">
                    <span>
                        <input type="radio" disabled name="option1">
                        <em class="icon-check"></em>
                    </span>
                    <div class="desc">公务卡：1300.00</div>
                </label>
                <label class="u-radio border-b">
                    <span>
                        <input type="radio" name="option1">
                        <em class="icon-check"></em>
                    </span>
                    <div class="desc">车贴：800.00</div>
                </label>
                <label class="u-radio border-b">
                    <span>
                        <input type="radio" name="option1">
                        <em class="icon-check"></em>
                    </span>
                    <div class="desc">个人现金：500.00</div>
                </label>
                <label class="u-radio border-b">
                    <span>
                        <input type="radio" name="option1">
                        <em class="icon-check"></em>
                    </span>
                    <div class="desc">关爱积分：2500.00</div>
                </label>
            </div>
        </div>
        <div> 
            <label class="u-checkbox">
                <span>
                    <input type="checkbox" name="option1">
                    <em class="icon-check"></em>
                </span>
                <div class="desc light">我同意关爱通使用我的已选资产在行程后自动支付</div>
            </label>
        </div>
        <div class="pop-slide-ft">
            <a type="submit" class="btn btn-primary" href="calltaxi.html">确认</a>
        </div>
    </section>
</section>

<section class="suggest"> 
    <div class="border-b option flex"> 
        <div class="city-select city-select-wrapper-hook border-r" style="width:82px;"> 
            <div class="city-select-hook">
                <span class="cityname" data-id="4">上海市</span>
                <i class="i-arrow-down"></i>
            </div> 
            <input class="city-input city-input-hook" type="text" placeholder="城市中文名或拼音" style="display: none;"> 
        </div> 
        <div class="address-select address-select-hook" style="left: 82px;"> 
            <input type="text" class="address-input address-input-hook" placeholder="您要去哪儿"> 
        </div> 
        <div class="cancel cancel-hook link">取消</div> 
    </div> 
    <div class="pos-wrapper pos-wrapper-hook"> 
        <ul class="pos-scroller-hook">
            <li data-index="0" class="border-b flex has-address">
            <span class="name name-hook ellipsis">虹桥机场</span>
            <span class="address address-hook ellipsis">虹桥机场</span>
            <input type="hidden" class="lat-hook" value="31.191649909374">
            <input type="hidden" class="lng-hook" value="121.34314769445">
            <!---->
            </li>

            <li data-index="1" class="border-b flex has-address">
            <span class="name name-hook ellipsis">浦东机场2号航站楼</span>
            <div class="address address-hook ellipsis">浦东机场2号航站楼浦东机场2号航站楼浦东机场2号航站楼浦东机场2号航站楼</div>
            <input type="hidden" class="lat-hook" value="31.153161300694">
            <input type="hidden" class="lng-hook" value="121.80464596945">
            <!---->
            </li>
        </ul> 
    </div> 
    <div class="city" style="display: none;">
        <div class="city-wrapper city-wrapper-hook">
            <div class="scroller-hook">
                <div class="current current-hook">当前城市：{{loc.defaultCity.cityName}}</div>
                <div class="cities cities-hook">
                    <div ng-repeat="cityGroup in cityGroups">
                        <div class="title">{{cityGroup.groupName}}</div>
                        <ul>
                            <li ng-repeat="city in cityGroup.cities" class="item border-b" data-name="{{city.cityName}}" data-id="{{city.cityId}}">
                                {{city.cityName}}
                            </li>
                        </ul>
                    </div>

                </div>
                <div class="shortcut shortcut-hook">
                    <ul>
                        <li data-anchor="★" class="item">★</li>
                        <li data-anchor="A" class="item">A</li>
                        <li data-anchor="B" class="item">B</li>
                        <li data-anchor="C" class="item">C</li>
                        <li data-anchor="D" class="item">D</li>
                        <li data-anchor="E" class="item">E</li>
                        <li data-anchor="F" class="item">F</li>
                        <li data-anchor="G" class="item">G</li>
                        <li data-anchor="H" class="item">H</li>
                        <li data-anchor="J" class="item">J</li>
                        <li data-anchor="K" class="item">K</li>
                        <li data-anchor="L" class="item">L</li>
                        <li data-anchor="M" class="item">M</li>
                        <li data-anchor="N" class="item">N</li>
                        <li data-anchor="P" class="item">P</li>
                        <li data-anchor="Q" class="item">Q</li>
                        <li data-anchor="R" class="item">R</li>
                        <li data-anchor="S" class="item">S</li>
                        <li data-anchor="T" class="item">T</li>
                        <li data-anchor="W" class="item">W</li>
                        <li data-anchor="X" class="item">X</li>
                        <li data-anchor="Y" class="item">Y</li>
                        <li data-anchor="Z" class="item">Z</li>
                    </ul>
                </div>
            </div>
            <div class="match match-hook">
                <ul class="match-scroller-hook"></ul>
            </div>
        </div>
    </div>

</section>







