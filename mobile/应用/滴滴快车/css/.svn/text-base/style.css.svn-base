html { font-family: "Helvetica Neue", Helvetica, STHeiTi, Arial, sans-serif; -ms-text-size-adjust: 100%; -webkit-text-size-adjust: 100%;  }
html,body{position: absolute; top: 0; left: 0; right: 0; bottom:0; width: 100%; height: 100%; overflow: hidden;}
body { margin: 0; font-size: 14px;   line-height: 1.42857; color: #333; background-color: #fff; -webkit-user-select: none; padding-bottom: 10px; -webkit-overflow-scrolling: touch; overflow-y: auto; overflow-x: hidden; }
.bg{background-color: #FAFAFA;}
section{ display: block; }
ul, li, div, h1, h2, h3, h4, h5, h6, p, form{ margin: 0; padding: 0; }
ul,li { list-style: none; }
body, a, img{ -webkit-touch-callout: none;}
a {text-decoration: none; cursor: pointer; color: #5EC5E8; outline: none; font-size: 12px; -webkit-tap-highlight-color:rgba(0,0,0,0);}
a:active{outline: none;}
img { border: 0; vertical-align: middle; }
h1,h2,h3,h4,h5{font-size: 100%;}
label{font-size:14px; color: #333; white-space:nowrap;}
button, input, select, textarea{color: inherit; font: inherit; margin: 0;  padding: 0; -webkit-tap-highlight-color:rgba(0,0,0,0); font-weight: normal; }
label{-webkit-tap-highlight-color:rgba(0,0,0,0); font-weight: normal; }
button:focus,input:focus,{ outline: none; -webkit-tap-highlight-color:rgba(0,0,0,0);} 
*[wx-forward-url]{cursor: pointer; -webkit-tap-highlight-color:rgba(0,0,0,0);}
button, select {text-transform: none; -webkit-appearance: none}
button, input[type="button"],input[type="number"], input[type="reset"], input[type="submit"] ,input[type="checkbox"]{ -webkit-appearance: none; cursor: pointer; border:none; border-radius: 0; }
button[disabled], html input[disabled] { cursor: default; }
input,select,textarea{font-size: 14px; line-height: normal; }
input,textarea{border:0;}
input[type="checkbox"], input[type="radio"] {padding: 0; }
input[type="number"]::-webkit-inner-spin-button, input[type="number"]::-webkit-outer-spin-button { height: auto; }
input[type="search"] {-webkit-appearance: textfield; }
input[type="search"]::-webkit-search-cancel-button, input[type="search"]::-webkit-search-decoration { -webkit-appearance: none; }
table { border-collapse: collapse; border-spacing: 0; }
input:-ms-input-placeholder, textarea:-ms-input-placeholder { color: #ccc; }
input::-webkit-input-placeholder, textarea::-webkit-input-placeholder { color: #ccc; }
*, *:after, *:before { -webkit-box-sizing: border-box; box-sizing: border-box;}
input:focus,textarea:focus {-webkit-tap-highlight-color: rgba(0,0,0,0);}
em{font-style: normal;}
textarea{resize: none; color: #666; width: 100%;}

.menu-item,div{-webkit-tap-highlight-color: rgba(0,0,0,0);}



/*项目图标*/
@font-face {font-family: "iconfont";
 src: url('../font/iconfont.eot');
 src: url('../font/iconfont.eot?#iefix') format('embedded-opentype'),
 url('../font/iconfont.woff') format('woff'), 
 url('../font/iconfont.ttf') format('truetype'),
 url('../font/iconfont.svg#iconfont') format('svg');
}        

[class^="icon-"]:before, [class*=" icon-"]:before{font-family:"iconfont" !important; font-style:normal; -webkit-font-smoothing: antialiased; -webkit-text-stroke-width: 0.2px; -moz-osx-font-smoothing: grayscale; display: inline-block; }

.icon-location-fill:before { content: "\e600"; }
.icon-star-fill:before { content: "\e601"; }
.icon-round-check-fill:before { content: "\e602"; }
.icon-round-close:before { content: "\e603"; }
.icon-check:before { content: "\e604"; }
.icon-round-uninstall:before { content: "\e605"; }
.icon-phone:before { content: "\e606"; }
.icon-right:before { content: "\e607";}
.icon-round-check:before { content: "\e608"; }
.icon-round-time:before { content: "\e60c"; }
.icon-round-zan:before { content: "\e60d"; }
.icon-round-complaint:before { content: "\e60e"; }
.icon-round-peak:before { content: "\e60f"; }


.container{padding-left: 12px; padding-right: 12px;}
.row{margin-left: -12px; margin-right: -12px;}


/*tips*/
.tips{padding-left: 12px; padding-right: 12px; background-color: #fff7b3; color: #666; font-size:12px; line-height:20px;}
.tips i[class*=icon-]{margin-right: 5px; float: left;}

/*按钮*/
.btn{width: 100%; display: block; position: relative; background-color: transparent; border:none; height: 42px; line-height: 42px; font-size: 16px; color: #fff; text-align: center; overflow: hidden; border-radius: 6px; -webkit-tap-highlight-color:rgba(0,0,0,0);}
.btn-default{box-shadow:0 0 1px 0 rgba(102,102,102,1); background-color: #fff; color: #666; }/*白色*/
.btn-default:active,.btn-default.active{box-shadow: 0 0 1px 0 rgba(0,0,0,1);}
.btn-primary{background-color: #ff814a;}/*首选橙色*/
.btn-primary:active{background-color: #fd773d;}
.btn-sm{height: 30px; line-height: 30px; font-size: 14px; border-radius: 4px;}
.btn-xs{height: 18px; line-height: 18px; font-size: 12px; border-radius: 3px; padding-left: 8px; padding-right: 8px; display: inline-block; width:auto;}
.btn-loading{background-color: #ccc; color: #fff;}
.btn.disabled,
.btn.disabled:active{background-color: #ccc !important; color: #fff !important;}
.btns-group{display:-webkit-box; display:box; padding-left: 6px; padding-right: 6px;}
.btns-group .btn{-webkit-box-flex:1; box-flex:1; margin: 0 6px;}
.btn-info-ghost{box-shadow:0 0 1px 0 rgba(2,176,202,1); background-color: #fff; color: #55c1e7; }/*蓝色幽灵按钮*/


/*单选，多选原件*/
.u-radio,.u-checkbox{font-size: 12px; cursor: pointer; padding-left: 20px; line-height: 16px; display: inline-block; vertical-align: middle; position: relative; word-break:break-all; vertical-align: middle;}
.u-radio .radio,.u-checkbox span{position: absolute; left:0; top: 50%; width: 16px; height: 16px; line-height: 14px; -webkit-transform:translateY(-50%); transform:translateY(-50%);}
.u-radio .radio em,.u-checkbox span em{ position: absolute; top: 0; left: 0; width:16px; height: 16px; overflow: hidden; cursor: pointer; border:1px solid #bbb;  text-align: center; font-size: 10px; -webkit-transition:.1s; transition:.1s;}
.u-radio .radio em{border-radius: 50%; }
.u-checkbox span em{border-radius: 0}
.u-radio .radio em:before,.u-checkbox span em:before{color: #fff}
.u-radio input,.u-checkbox input{display: none}
.u-radio input:checked+em,.u-checkbox input:checked+em{border-color: #3bb200; vertical-align: middle; background-color: #3bb200; color: #fff}
.m-radio .u-radio{display: block; width: 100%; color: #666; text-align: left;  line-height: 16px; margin-top: 8px; }

/* 边框特殊处理 */
.border-l,.border-r,.border-t,.border-b{position: relative;}
.border-l:before, .border-r:after, .border-t:before, .border-b:after{content: ''; position: absolute; overflow: hidden; border-style:solid; border-color:#E8E8E8;}
.border-l:before, .border-r:after{width: 0; height: 100%; border-width:0 1px 0 0; -webkit-transform:scaleX(0.5); transform:scaleX(0.5);}
.border-t:before, .border-b:after{width: 100%; height: 0; border-width:1px 0 0 0; -webkit-transform:scaleY(0.5); transform:scaleY(0.5);}
.border-l:before,.border-t:before{top:0; left: 0;}
.border-r:after,.border-b:after{bottom:0; right: 0;}
.border-dashed:after,.border-dashed:before{ border-style: dashed; }

/*盒模型*/
.flex{display:-webkit-box; display:box;}
.ellipsis{display:-webkit-box; display:box; overflow : hidden; text-overflow: ellipsis; -webkit-line-clamp: 1; line-clamp: 1; -webkit-box-orient: vertical; box-orient: vertical;}

/*form-group*/
.form-group{position: relative; height: 44px; padding-top:4px; padding-bottom: 4px; width: 100%; display: -webkit-box; display: box; -webkit-box-align:center; box-align:center;} 
.form-label{font-size: 14px; color: #333; width: 82px; padding-right: 12px; white-space:nowrap; display: -webkit-box; display: box; -webkit-box-align:center; box-align:center;}
.form-label.narrow{width: 70px;}
.form-control-box{position: relative; -webkit-box-flex:1; box-flex:1;}
.form-control{border:none; height:36px; background-color: transparent; padding:0 24px 0 6px; width: 100%; vertical-align: middle;}
select.form-control{position: relative; z-index: 2;}
.form-control-verify{padding-right: 125px;}
.form-control.verify{padding-right: 6px;}
.form-control-right{position: absolute; right: 0; top: 50%; z-index: 0; text-align: center;-webkit-transform:translateY(-50%); transform:translateY(-50%);}
.form-control-right[class*=icon]{width: 24px; height: 24px; line-height:24px; text-align: center; vertical-align: middle; font-size: 16px; border-radius: 50%; -webkit-tap-highlight-color:rgba(0,0,0,0);}
.form-control-right.icon-close:active{background-color: #bbb; color: #fff;-webkit-transition:.2s; transition:.2s;}
.form-control-right.icon-right:active{color: #333; }
.form-code{width: 90px;}
.form-code .btn-info-ghost[disabled]{background-color: #ccc; color: #fff; box-shadow:none;}

/*弹出层*/
.popup-mask,.pop-slide-up{background-color: rgba(0,0,0,.3); position: fixed; width: 100%; height: 100%; overflow: hidden; display: none; top: 0; left: 0;  z-index: 10}
.popup-mask{z-index: 99;}
.pop-slide-con{position: absolute; top: 100%; left: 0; width: 100%; -webkit-transition:.2s; transition:.2s; overflow: hidden;}
.pop-slide-con-list{ -webkit-box-align:end; box-align:end; -webkit-transition:.2s; transition:.2s;}
.pop-slide-con-item{padding:0 12px 12px; width: 100%; background-color: #fff; }
.pop-slide-hd{position: relative; line-height: 20px; text-align: center; padding: 16px 40px;}
.pop-slide-hd h3{ font-size: 16px;}
.pop-slide-hd p{ color: #666;}
.pop-slide-bd{padding-bottom: 16px; }
.pop-slide-hd .close{position: absolute; left: 8px; top: 14px; font-size: 24px; width: 26px; height: 26px; line-height: 26px; overflow: hidden; text-align: center; color:#bbb;}

.popup-container{width: 74%; position: fixed; top: 40%; left: 50%; display: none; overflow: hidden; border-radius: 12px;  text-align: center; z-index: 999;	-webkit-transform:translateX(-50%) translateY(-50%); transform:translateX(-50%) translateY(-50%);}
.popup-con{width: 100%; background-color: #fff; overflow: hidden; border-radius: 12px; padding-top: 16px; }
.popup-hd{font-size: 15px; text-align: center; color: #333; font-weight: bold; margin-bottom: 5px;}
.popup-bd{padding: 4px 20px 20px 20px; text-align: center; line-height: 18px; font-size: 12px; }
.popup-ft,.popup-ft>a{display: -webkit-box; display: box; -webkit-box-align:center; box-align:center;}
.popup-ft{background-color: #fff; height: 44px; overflow: hidden; border-radius:0 0 12px 12px; }
.popup-ft>a{ height: 44px; color: #2181f7; font-size: 15px; font-weight: bold; -webkit-box-flex:1; box-flex:1; -webkit-box-pack:center; box-pack:center; -webkit-tap-highlight-color:rgba(0,0,0,0);} 
.popup-ft>a:active{color: #0072ff;}
.popup-con-notfiy{ position: fixed; top: 40%; left: 50%; min-width: 100px; display: none; border-radius: 12px; padding: 10px 15px; opacity:0; background-color: rgba(0,0,0,0.8); color: #fff; -webkit-transition:.2s opacity; transition:.2s opacity; z-index: 999; -webkit-transform:translateX(-50%) translateY(-50%); transform:translateX(-50%) translateY(-50%); text-align: center;}
.popup-con-notfiy .popup-bd{padding-top: 15px; padding-bottom: 15px;}

.clearfix:after{display: block; clear: both; visibility: hidden; height: 0; overflow:hidden; content:" ";}

/*index*/
