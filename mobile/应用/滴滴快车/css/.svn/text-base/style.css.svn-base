.status-info{margin: 25px; text-align: center;}
.status-detail{background-color: #fff; border-radius: 4px; box-shadow:0 0 1px 0 rgba(204,204,204,1);}
.status-detail-hd{padding: 10px 12px; -webkit-box-align:center; box-align:center;}
.status-detail-hd .distance,.status-detail-hd .time{-webkit-box-flex:1; box-flex:1; text-align: center; height: 40px; line-height: 40px;}
.status-detail-bd{padding: 15px 12px;}

.status-cost{-webkit-box-align:center; box-align:center; -webkit-box-pack:center; box-pack:center;}
.status-cost .icon-right{font-size: 16px; color: #bbb; margin-left: 5px;}
.status-cost-con .cost{line-height: 1; font-size: 16px;}
.status-cost-con .notice{font-size: 12px; color: #999;}

.pay-info-hd{text-align: center;}
.pay-info-hd .title{display: inline-block; position: relative; padding: 0 10px; font-size: 12px; color: #999; line-height: 1; top: 13px; background-color: #FAFAFA; z-index: 2;}
.pay-info-hd:after{content: ''; display: block; z-index: 1; top:22px;}

.suggestLoading{height: 16px; line-height: 16px; color: #999; font-size: 12px; text-align: center;}
.suggestLoading .loadImg{display: inline-block; width: 16px; height: 16px; overflow: hidden; background: url(../img/loading.gif) no-repeat; -webkit-background-size: 16px auto; background-size: 16px auto; margin-right: 6px; vertical-align: middle;}


.order-wrap{background-color: #FAFAFA; position: absolute; width: 100%; height:100%; left: 0; top: 0; bottom: 0;}
.no-addressData{font-size: 20px; color: #999; position: absolute; top: 100px; left: 50%; -webkit-transform: translateX(-50%); transform: translateX(-50%);}


.star-rating{text-align: center; margin-top: 32px;}
.star-rating .star{display: inline-block; width: 34px; height: 34px; overflow: hidden; background: url(../img/star.png) no-repeat; -webkit-background-size: 34px auto; background-size: 34px auto; margin: 0 6px;}
.star-rating.unusable .star{background-position: 0 -68px;}
.star-rating .star.active{background-position: 0 -34px;}


.evaluate-label{margin-left: -6px; margin-right: -6px; margin-top: 8px;}
.evaluate-label-item{float: left;   margin: 0 6px 12px; position: relative;}
.evaluate-label-item .con{height: 30px; line-height: 30px; box-shadow:0 0 1px #000; border-radius: 15px; padding-left: 12px; padding-right: 12px; color: #999; font-size: 14px; transition:.2s;}
.evaluate-label-item .icon-round-zan{font-size: 18px; float: right; line-height: 28px; position: relative; left: 8px;}
.evaluate-label-item input{position: absolute; left: -999em;}
.evaluate-label-item input:checked+.con{color: #f60; box-shadow:0 0 1px #e54600;}
.evaluate-label-item.low .icon-round-zan{transform: rotateX(-180deg);}
.evaluate-textarea{height: 100px; padding: 8px; font-size: 14px; box-shadow:0 0 1px #999; border-radius: 4px; }
.evaluate-txt{font-size: 14px; padding: 8px; background-color: #F4F4F4; color: #aaa;}

.complaint-item{box-shadow:0 0 1px #999; padding-top: 12px; padding-bottom: 12px; border-radius: 4px; background-color: #fff; margin-top: 12px; color: #999; position: relative;}
.complaint-item input{position: absolute; left: -999em;}
.complaint-item-hd,.complaint-item-bd{padding-left: 12px; padding-right: 12px;}
.complaint-item-hd{color: #999; display: block; height: 24px; line-height: 24px;}
.complaint-item-hd i{font-size: 18px; float: right;}
.complaint-item-bd{font-size: 12px; line-height: 20px; margin-top: 2px; display: none;}
.complaint-item-bd .info{line-height: 20px; padding-bottom: 8px;}
.complaint-item-bd textarea{margin-top: 8px; font-size: 12px; height: 50px;}
.complaint-item input:checked+.complaint-item-con .icon-round-check-fill{color: #f60;}
.complaint-item input:checked+.complaint-item-con .complaint-item-bd{display: block;}

