<script type="text/javascript">
	$(function(){
		var oBtn = $('.topbar-search-mall .icon-tabulation');
		var oSilderBar = $('.silderBar-mall');
		var oList = oSilderBar.find('.list');
		var i=0;

		oBtn.add('.silderBar-mall').click(function(event) {
		
			if(i%2==0){
				oSilderBar.show();
				if(i==0){
					var scrollbar = new IScroll('#iScroll-bar-mall',{
						preventDefault: false,
						click:true
					});
				}
				setTimeout(function(){
					oList[0].style.transform = 'translateX(0)';
					oList[0].style.webkitTransform = 'translateX(0)';
					i++;
				},0)
			}else{
				oList[0].style.transform = 'translateX(-100%)';
				oList[0].style.webkitTransform = 'translateX(-100%)';

				setTimeout(function(){
					oSilderBar.hide();
					i++;
				},200)
			}
			
		});
	})
</script>
