<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<title>页面标题</title>
	<meta http-equiv="Cache-Control" content="no-siteapp" /><!-- 百度禁止转码 -->
	<meta name="keywords" content="这里写关键字">
	<meta name="description" content="这里写页面描述">
	<meta name ="viewport" content ="width=device-width, initial-scale=1, maximum-scale=1, minimum-scale=1, user-scalable=no"> 
	<meta name="format-detection" content="telephone=no,email=no" /> <!-- 禁止自动识别为电话号码和邮件 -->
	<script src="../public/js/jquery.js"></script>
	<link rel="stylesheet" href="../public/css/base.css">
</head>
<body>

<section class="container">
	<section class="form-group">
		<label class="form-label" for="userName">收货人</label>
		<div class="form-control-box" wx-role="empty-input">
			<input class="form-control" id="userName" type="number" value="" placeholder="请输入收货人姓名">
		</div>
	</section>
	<section class="form-group">
		<label class="form-label" for="phoneNum">手机号码</label>
		<div class="form-control-box" wx-role="empty-input">
			<input class="form-control" id="phoneNum" type="number" value="" placeholder="请输入11位手机号码">
		</div>
	</section>
	<section class="form-group">
		<label class="form-label">省</label>
		<div class="form-control-box">
			<label>
				<select class="form-control" name="select1" id="select1"><option value="">上海</option><option value="">北京</option></select>
				<i class="form-control-right icon-right"></i>
			</label>
		</div>
	</section>
	<section class="form-group">
		<label class="form-label">市</label>
		<div class="form-control-box">
			<label>
				<select class="form-control" name="select2" id="select2"><option value="">北京市</option></select>
				<i class="form-control-right icon-right"></i>
			</label>
		</div>
	</section>
	<section class="form-group">
		<label class="form-label">区</label>
		<div class="form-control-box">
			<label>
				<select class="form-control" name="select3" id="select3"><option value="">东城区</option><option value="">西城区</option></select>
				<i class="form-control-right icon-right"></i>
			</label>
		</div>
	</section>
	<section class="form-group">
		<div class="form-control-box" wx-role="empty-input">
			<input class="form-control" id="userName" type="number" value="" placeholder="请输入详细地址">
		</div>
	</section>
	<button type="submit" class="btn btn-primary mt50">保存</button>
</section>
	<script src="../public/js/base.js"></script>
</body>
</html>
